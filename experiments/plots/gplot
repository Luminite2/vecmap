set pointsize 2
set style data lines
set style line 1 dt 2 lc rgb "#000000" lw 1
set style line 2 dt 3 lc rgb "#4B0082" lw 1
set style line 3 dt 5 lc rgb "#800000" lw 1
set output "1gram_dev.png"
set yrange [0:60]
plot "1gram.data" using 1:2 ls 1 w lp title 'English-German', \
"1gram.data" using 1:6 ls 2 w lp title 'English-Italian', \
"1gram.data" using 1:4 ls 3 w lp title 'English-Finnish'
